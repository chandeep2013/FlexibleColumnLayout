{
    "welcomeFile": "/index.html",
    "authenticationMethod": "route",
    "routes": [
        {
            "authenticationType": "xsuaa",
            "csrfProtection": false,
            "source": "^/user-info/UserInfoEntity",
            "destination": "CAPM_LOGGED_IN_USER_INFO"
        },
        {
            "authenticationType": "xsuaa",
            "csrfProtection": false,
            "source": "^/getOAuthToken",
            "destination": "node-destination-oauth-token"
        },
        {
            "authenticationType": "none",
            "csrfProtection": false,
            "source": "^/drop-down(.*)$",
            "destination": "CAPM_DROP_DOWN"
        },{
            "authenticationType": "none",
            "csrfProtection": false,
            "source": "^/my-dms/getTreeTable(.*)$",
            "destination": "CAPM_DMS"
        },
        {
            "source": "^/DMS_API/(.*)$",
            "target": "/DMS_API/$1",
            "destination": "DMS_API",
            "authenticationType": "none",
            "csrfProtection": false
        },{
            "source": "^/sap/(.*)$",
            "target": "/sap/$1",
            "destination": "MHIRJ_S4HANA",
            "authenticationType": "xsuaa",
            "csrfProtection": false
        },
        {
            "source": "^/V1/API_BUSINESS_PARTNER/(.*)$",
            "target": "/V1/API_BUSINESS_PARTNER/$1",
            "destination": "DMS_API_IMPERSONATE_USER",
            "authenticationType": "none",
            "csrfProtection": false
        },        
        {
            "source": "^(.*)$",
            "target": "$1",
            "service": "html5-apps-repo-rt",
            "authenticationType": "xsuaa"
        }
    ]
}