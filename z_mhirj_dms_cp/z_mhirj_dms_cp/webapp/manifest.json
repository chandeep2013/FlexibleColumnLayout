{
    "_version": "1.32.0",
    "sap.app": {
        "id": "com.mhirj.dms.zmhirjdmscp",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "DOCUMENTS LIBRARY",
        "description": "Browse and view all document types",
        "dataSources": {
            "DMS_SAP_Data_Source": {
                "uri": "/sap/opu/odata/sap/ZZ1_DMS_NOTIF_SUB_CDS/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "FLEET_SAP_Data_Source": {
                "uri": "/sap/opu/odata/sap/ZAPI_BP_FLEET_INF_SRV",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }            
        },
        "crossNavigation": {
            "inbounds": {
                "intent1": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "DMSLIBCPSemObj",
                    "action": "DMSLIBCUS",
                    "title": "DOCUMENTS LIBRARY",
                    "info": "",
                    "subTitle": "Browse and view all document types",
                    "icon": "sap-icon://documents"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "rootView": {
			"viewName": "com.mhirj.dms.zmhirjdmscp.view.App",
			"type": "XML",
			"async": true,
			"id": "fcl"
		},
        "dependencies": {
            "minUI5Version": "1.102.1",
            "libs": {
                "sap.ui.core": {},
                "sap.f":{},
                "sap.ui.layout":{}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.mhirj.dms.zmhirjdmscp.i18n.i18n"
                }
            },
            "DMS_SAP_Data_Model": {
                "dataSource": "DMS_SAP_Data_Source",
                "preload": true,
                "settings": {}
            },
            "FLEET_SAP_Data_Model":{
                "dataSource": "FLEET_SAP_Data_Source",
                "preload": true,
                "settings": {}
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.f.routing.Router",
                "type": "View",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.mhirj.dms.zmhirjdmscp.view",
                "controlId": "flexibleColumnLayout",
				"transition": "slide",
				"bypassed": {
				}
            },
			
            "routes": [
                {
					"pattern": ":layout:",
					"name": "RouteTreeScreen",
					"target": [
						"TargetTreeScreen",
						"TargetDetailScreen"
					]
				},
                {
					"pattern":"detail/{docGroup}/{DocumentType}/{layout}",
					"name": "RouteDetailScreen",
					"target": [
						"TargetTreeScreen",
						"TargetDetailScreen"
					]
				},
                {
					"pattern":"detail/{docGroup}/detailDetail/{DocumentType}/{UNID}/{Navigation}/{layout}",
					"name": "RouteDetailDetailScreen",
					"target": [
						"TargetTreeScreen",
						"TargetDetailScreen",
                        "TargetDetailDetailScreen"
					]
				},
                {
                    "pattern":"detailScreen/{docSetId}",
					"name": "RouteOthDetailScreen",
					"target": [
                        "TargetOthDetailScreen"
					]
                }

            ],
            "targets": {
                "TargetTreeScreen": {
                    "viewName": "TreeList",
                    "viewId": "TreeList",
					"controlAggregation": "beginColumnPages"
				},
				"TargetDetailScreen": {
					"viewName": "Detail",
                    "viewId": "Detail",
					"controlAggregation": "midColumnPages"
				},
                "TargetDetailDetailScreen": {
					"viewName": "DetailDetail",
                    "viewId": "DetailDetail",
					"controlAggregation": "endColumnPages"
				},
                "TargetOthDetailScreen": {
					"viewName": "DetailedScreen",
                    "viewId": "DetailedScreen"
				}
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "com-mhirj-dms-zmhirjdmscp"
    }
}