							<FlexBox direction="Column" class="sapUiSmallMarginBottom">
								<Label text="{i18n>detailedScreenReleaseDate}" design="Bold" />
								<Label text="{LocalDataModel>/DMS_Model/DocSetDetails/0/ReleaseDate}" tooltip="{LocalDataModel>/DMS_Model/DocSetDetails/0/ReleaseDate}"/>
							</FlexBox>
                            <FlexBox direction="Column" class="sapUiSmallMarginBottom">
								<Label text="{i18n>detailedScreenDocType}" design="Bold" />
