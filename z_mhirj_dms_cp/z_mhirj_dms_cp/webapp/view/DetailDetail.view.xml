							</FlexBox>
							<FlexBox direction="Column" class="sapUiSmallMarginBottom">
								<Label text="{i18n>detailedScreenTitleOfTheDocSet}" design="Bold" />
								<Label text="{LocalDataModel>/DMS_Model/DocSetDetails/0/DocSetTitle}" tooltip="{LocalDataModel>/DMS_Model/DocSetDetails/0/DocSetTitle}"/>
							</FlexBox>
							<FlexBox direction="Column" class="sapUiSmallMarginBottom">
								<Label text="{i18n>detailedScreenDesc}" design="Bold" />
								<Label text="{LocalDataModel>/DMS_Model/DocSetDetails/0/Description}" tooltip="{LocalDataModel>/DMS_Model/DocSetDetails/0/Description}"/>
							</FlexBox>
