							</FlexBox>
							<FlexBox direction="Column" class="sapUiSmallMarginBottom">
								<Label text="{i18n>detailedScreenDocNumber}" design="Bold" />
								<Label text="{LocalDataModel>/DMS_Model/DocSetDetails/0/DocNumber}" tooltip="{LocalDataModel>/DMS_Model/DocSetDetails/0/DocNumber}"/>
							</FlexBox>
							<FlexBox direction="Column" class="sapUiSmallMarginBottom" visible="{LocalDataModel>/DMS_Model/DetailVisibleFields/RevisionNumber}">
