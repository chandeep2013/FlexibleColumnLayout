							</FlexBox>
							<FlexBox direction="Column" class="sapUiSmallMarginBottom">
								<Label text="{i18n>detailedScreenAttachments}" design="Bold" />
								<Table items="{LocalDataModel>/DMS_Model/DocSetDetails/0/fileinfo}" >
									<columns>
										<Column width="10%">
											<Label text="" />
										</Column>
										<Column width="70%">
											<Label text="" />
										</Column>
										<Column width="10%">
											<Label text="" />
										</Column>
										<Column width="10%">
											<Label text="" />
										</Column>
									</columns>
									<ColumnListItem>
										<core:Icon src="sap-icon://activity-2" color="#031E48" />
										<Label text="{LocalDataModel>FileName}" tooltip="{LocalDataModel>FileName}"/>
										<Label text="{LocalDataModel>FileSize}" tooltip="{LocalDataModel>FileSize}"/>
										<core:Icon src="sap-icon://download" color="#031E48" press="downloadAttachment" />
									</ColumnListItem>
