<mvc:View 
    controllerName="com.mhirj.dms.zmhirjdmscp.controller.TreeList"
    xmlns="sap.m"
	xmlns:f="sap.f"
    xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc" 
    xmlns:semantic="sap.f.semantic"> 
    <f:DynamicPage id="dynamicPageId" toggleHeaderOnTitleClick="false">
        <f:title>
			<f:DynamicPageTitle>
				<f:heading>
					<Title text="DOCUMENTS LIBRARY" />
				</f:heading>
                <f:actions>
                     <Button icon="sap-icon://bell" type="Transparent" tooltip="Manage Notifications Preferences" press="openEmailNotificationDialog"/>
                </f:actions>
            </f:DynamicPageTitle>
		</f:title>
        <f:content>
            <!-- <Tree id="idTree" items="{path: 'LocalDataModel>/DMS_Model/TreeModel'}" mode="SingleSelectMaster" selectionChange="selectedTreeItemPress">
				<StandardTreeItem title="{LocalDataModel>name}" tooltip="{LocalDataModel>name} ({LocalDataModel>code})" class="tree" type="Navigation" press="selectedTreeItemPress" />
			</Tree> -->

            <Tree id="idTree" items="{path: 'LocalDataModel>/DMS_Model/TreeModel'}" >
                <StandardTreeItem title="{LocalDataModel>name}" tooltip="{LocalDataModel>name} ({LocalDataModel>code})" class="tree" type="{= ${LocalDataModel>parent}.length > 0 ? 'Navigation' : 'Inactive' }" press="selectedTreeItemPress" />
            </Tree>
        </f:content>
    </f:DynamicPage>
</mvc:View>
